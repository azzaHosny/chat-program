<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" onMouseDragged="#draggAction" onMousePressed="#onMouseClicked" prefHeight="605.0" prefWidth="534.0" style="-fx-background-color: #1DA1F2;" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SignUpController">
    <stylesheets>
        <URL value="@signup.css" />
    </stylesheets>
    <children>
        <TextField fx:id="firstName" layoutX="55.0" layoutY="97.0" prefHeight="42.0" prefWidth="424.0" promptText="Enter Your First Name ....">
            <font>
                <Font name="System Bold Italic" size="16.0" />
            </font>
        </TextField>
        <TextField fx:id="lastName" layoutX="52.0" layoutY="171.0" prefHeight="42.0" prefWidth="424.0" promptText="Enter Your Last Name ....">
            <font>
                <Font name="System Bold Italic" size="16.0" />
            </font>
        </TextField>
        <TextField fx:id="email" layoutX="52.0" layoutY="242.0" prefHeight="42.0" prefWidth="424.0" promptText="Enter Your Email ....">
            <font>
                <Font name="System Bold Italic" size="16.0" />
            </font>
        </TextField>
        <TextField fx:id="password" layoutX="52.0" layoutY="317.0" prefHeight="42.0" prefWidth="424.0" promptText="Enter Your Password ....">
            <font>
                <Font name="Arial Bold Italic" size="16.0" />
            </font>
        </TextField>
        <RadioButton fx:id="male" layoutX="139.0" layoutY="398.0" mnemonicParsing="false" text="male" textFill="#fff7f7">
            <font>
                <Font size="19.0" />
            </font>
        </RadioButton>
        <RadioButton fx:id="female" layoutX="245.0" layoutY="398.0" mnemonicParsing="false" text="female" textFill="#f8f4f4">
            <font>
                <Font size="19.0" />
            </font>
        </RadioButton>
        <Label layoutX="65.0" layoutY="391.0" prefHeight="42.0" prefWidth="59.0" text="Gender" textFill="#f2eeee">
            <font>
                <Font name="System Bold Italic" size="16.0" />
            </font>
        </Label>
        <Label layoutX="55.0" layoutY="373.0" />
        <ComboBox fx:id="country" layoutX="263.0" layoutY="465.0" prefHeight="25.0" prefWidth="203.0" promptText="Egypt" />
           
        <Label layoutX="65.0" layoutY="465.0" prefHeight="25.0" prefWidth="169.0" text="Choose Your Country" textFill="#eeebeb">
            <font>
                <Font name="System Bold Italic" size="16.0" />
            </font>
        </Label>
        <Button fx:id="register" layoutX="65.0" layoutY="516.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="405.0" style="-fx-background-color: #486380;" text="Register" textFill="#f5f3f3">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Button>
        <ImageView fx:id="close" fitHeight="30.0" fitWidth="30.0" layoutX="479.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
        <ImageView fx:id="minimize" fitHeight="30.0" fitWidth="30.0" layoutX="440.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
    </children>
</AnchorPane>
